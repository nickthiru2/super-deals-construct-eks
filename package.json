  "author": "Nick Thiru <nick.thiru@gmail.com>",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://ms-324093102745.d.codeartifact.us-east-1.amazonaws.com/npm/eks-construct/"
